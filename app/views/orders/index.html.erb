<h1>Listing orders</h1>

<table>
  <thead>
    <tr>
      <th>名字</th>
      
      <th>联系方式</th>
       <th></th>
      <th>送货日期</th>
      <th></th>
      <th></th>
      <th>送货时间</th>
     <!--  <th>Email</th>
      <th>Pay type</th> -->
      <th>user</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= order.name %></td>
        <td><%= order.phonenumber %></td>
        <td></td>
        <td><%= order.date %></td>
        <td></td>
        <td></td>
        <td><%= order.time %></td>
        <td><%= order.user_id %></td>
        <td><%= link_to '查看详情', order %></td>
        <td><%= link_to '编辑', edit_order_path(order) %></td>
        <td><%= link_to '删除', order, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<p><%= will_paginate @orders %></p>
